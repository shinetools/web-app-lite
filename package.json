{
  "name": "@shinetools/webapp",
  "version": "1.0.0",
  "main": "server.js",
  "author": "Shine",
  "license": "UNLICENSED",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "check-types": "tsc --noEmit",
    "start": "node ./dist/server",
    "get-schema": "node ./scripts/getSchema.js",
    "dev:client": "webpack-dev-server --config webpack/webpack.dev.js --open --hot",
    "dev:server": "nodemon --watch ./src/server.ts --exec 'ts-node' ./src/server.ts",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build": "tsc --project ./tsconfig.server.json && NODE_ENV=production webpack --config webpack/webpack.prod.js",
    "test": "NODE_ICU_DATA=./node_modules/full-icu/ jest --maxWorkers=2",
    "test:watch": "yarn test --watch",
    "test:e2e": "./scripts/launchE2eTest.sh",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "prettify": "prettier-eslint --write \"src/*.ts*\" --list-different",
    "stop": "pkill ShineWebApp",
    "deploy:production": "API_ENV=production FEATURES_ENV=production yarn build && gcloud app deploy --project=shine-front --quiet",
    "deploy:staging": "API_ENV=staging FEATURES_ENV=staging yarn build && gcloud app deploy --project=shine-front-staging --quiet"
  },
